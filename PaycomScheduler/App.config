<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <appSettings>
    <add key="PaycomApiBaseURL" value="http://test.afiniti.com/paycomAPItest/ProcessPaycom/" />
    
    <add key="MaximumRequests" value="1" />
    <add key="LogTo" value="1" />
    <add key="FileName" value="PaycomEngineSchedulertest" />
    <add key="FilePath" value="C:\Logs\" />
  </appSettings>

  <system.diagnostics>
    <switches>
      <add name="EnableTracing" value="1" />
    </switches>
    <trace>
      <listeners>
        <add name="ServMsg" />
      </listeners>
    </trace>
    <sharedListeners>
      <add name="ServMsg" type="Afiniti.Framework.LoggingTracing.Tracing, Afiniti.Framework.LoggingTracing, Version=1.0.0.0, Culture=neutral, PublicKeyToken=62fd2d71e5c3e62c" />
    </sharedListeners>
  </system.diagnostics>

  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="NPOI" publicKeyToken="0df73ec7942b34e1" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-2.5.1.0" newVersion="2.5.1.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="NPOI.OOXML" publicKeyToken="0df73ec7942b34e1" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-2.5.1.0" newVersion="2.5.1.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>